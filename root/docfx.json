{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/SadConsole.dll",
            "src/SadConsole.Extended.dll",
            "src/SadConsole.Host.MonoGame.dll"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ]
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "apidocs",
        "dest": "api"
      },
      {
        "files": [ "articles/**.{md,yml}", "toc.yml", "*.md" ],
        "exclude": ["**/include/**", "**/**.temp.md"]
      }
    ],
    "resource": [
      {
        "files": [
          "_config.yml",
          "images/**",
          "articles/**/images/**.png",
          "articles/**/images/**.gif",
          "articles/**.zip"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../_site",
    "globalMetadata": {
      "_appTitle": "SadConsole"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "../templates/boot386"
    ],
    "fileMetadata": {
      "titleSuffix": {
        "articles/**.md": "SadConsole v10"
      }
    },
    "xrefservice": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "xref": [
      "https://thesadrogue.github.io/TheSadRogue.Primitives/xrefmap.yml"
    ],
    "sitemap":{
      "baseUrl": "https://sadconsole.com",
      "priority": 0.5,
      "changefreq": "monthly",
      "fileOptions":{
        "**/api/**.yml": {
          "priority": 0.7
        },
        "**/articles/**.md": {
            "priority": 1.0,
            "changefreq": "monthly"
        }
      }
    },
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}